ARG tag
FROM maven:3.6.3-jdk-8 AS build-stage
RUN whoami
RUN git clone https://github.com/lorenyeung/java-maven-junit-helloworld
WORKDIR java-maven-junit-helloworld
RUN mkdir /root/.m2
ADD /root/.m2 /root/.m2
RUN mvn test

